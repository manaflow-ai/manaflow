    "test:watch": "vitest"
  },
  "dependencies": {
    "@freestyle-sh/with-bun": "^0.2.0",
    "freestyle-sandboxes": "^0.1.6",
    "morphcloud": "^0.0.20"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "vitest": "^4.0.16"
  }
}
